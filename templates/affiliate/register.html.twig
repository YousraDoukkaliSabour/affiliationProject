{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
    <h1>Register</h1>
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.phoneNumber) }}
    {{ form_row(form.address) }}
    <button type="submit">Register</button>
    {{ form_end(form) }}

    <p>Already have an account? <a href="{{ path('affiliate_login') }}">Login</a></p>
{% endblock %}
