{# templates/affiliate/login.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Login as an Affiliate{% endblock %}

{% block body %}
    <h1>Login as an Affiliate</h1>
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}
    <form method="post">
        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.submit) }}
        {{ form_end(form) }}
    </form>
{% endblock %}
